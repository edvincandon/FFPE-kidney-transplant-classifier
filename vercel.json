{
  "version": 2,
  "functions": {
    "api/index.py": {
      "memory": 3008,
      "maxDuration": 30,
      "includeFiles": "{api/static/**/*, api/templates/**/*, api/_classifier.py, models/**/*}"
    }
  },
  "rewrites": [
    { "source": "/", "destination": "/api" },
    { "source": "/:match*", "destination": "/api" }
  ]
}
