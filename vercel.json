{
  "version": 2,
  "functions": {
    "api/index.py": {
      "memory": 3008,
      "maxDuration": 30,
      "includeFiles": "{api/static/**/*, api/templates/**/*, api/_classifier.py, models/**/*}"
    }
  },
  "routes": [
    { "src": "/", "dest": "api/index.py" },
    { "src": "/static/**", "dest": "api/index.py" }
  ]
}
